
{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "survey-template-schema",
    "title": "Survey Template",
    "description": "A generic survey template",
    "type": "object",

    "properties": {
        "type": {
            "type":"string",
            "description": "Name of the new template"
        },
        "questionIds": {
            "type":"array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": ["type", "questionIds"]
}
